{
  "name": "contributte/elastica",
  "type": "library",
  "description": "Elastica implementation for Nette Framework",
  "keywords": [
    "elastica",
    "elasticsearch",
    "nette",
    "elastic",
    "search",
    "es"
  ],
  "homepage": "https://github.com/contributte/elastica",
  "license": "MIT",
  "authors": [
    {
      "name": "Dalibor Korpar",
      "email": "dalibor.korpar@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.1",
    "ruflin/elastica": "^7.3.1",
    "nette/di": "^3.1.8",
    "nette/utils": "^4.0.3"
  },
  "require-dev": {
    "tracy/tracy": "^2.10.5",
    "nette/http": "^3.2.3",
    "nette/bootstrap": "^3.2.1",
    "mockery/mockery": "^1.6.6",
    "contributte/qa": "^0.4",
    "contributte/tester": "^0.4",
    "contributte/phpstan": "^0.1"
  },
  "conflict": {
    "nette/schema": "<1.2.0"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\Elastica\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.1.0-dev"
    }
  }
}
