{
  "name": "contributte/elastica",
  "type": "library",
  "description": "Elastica implementation for Nette Framework",
  "keywords": [
    "elastica",
    "elasticsearch",
    "nette",
    "elastic",
    "search",
    "es"
  ],
  "homepage": "https://github.com/contributte/elastica",
  "license": "MIT",
  "authors": [
    {
      "name": "Dalibor Korpar",
      "email": "dalibor.korpar@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.2",
    "ruflin/elastica": "^7.0",
    "nette/di": "^3.0",
    "nette/utils": "^3.0"
  },
  "require-dev": {
    "tracy/tracy": "^2.6",
    "nette/http": "^3.0",
    "nette/bootstrap": "^3.0",
    "phpunit/phpunit": "^7.0|8.0|^9.0",
    "ninjify/qa": "^0.12",
    "phpstan/phpstan": "^1.0",
    "phpstan/phpstan-deprecation-rules": "^1.0",
    "phpstan/phpstan-nette": "^1.0",
    "phpstan/phpstan-strict-rules": "^1.0"
  },
  "conflict": {
    "nette/schema": "<1.2.0"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\Elastica\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.2.x-dev"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
